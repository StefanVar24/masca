{
  "name": "ssi-snap",
  "version": "1.0.0",
  "description": "Snap for SSI DAO",
  "main": "index.js",
  "scripts": {
    "start": "yarn run build && yarn run serve",
    "build": "yarn run build:tsc && yarn run build:snap",
    "rebuild": "mm-snap manifest --fix && mm-snap eval && mm-snap serve",
    "build:snap": "yarn build:snap:bundle && yarn build:snap:postprocess && yarn build:snap:eval",
    "build:snap:bundle": "mm-snap build --verboseErrors  --stripComments --eval false",
    "build:snap:postprocess": "node ./post-process.js && mm-snap manifest --fix",
    "build:snap:eval": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "build:tsc": "tsc --build --pretty",
    "serve": "mm-snap serve",
    "fix": "mm-snap manifest --fix",
    "postinstall": "patch-package"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.18",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@metamask/eth-sig-util": "^4.0.0",
    "did-jwt-vc": "^2.1.9",
    "ethers": "^5.5.4",
    "ethr-did": "^2.2.0",
    "ethr-did-resolver": "^5.0.4",
    "js-base64": "^3.7.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "uuid": "^8.3.2",
    "web-did-resolver": "^2.0.12"
  }
}
